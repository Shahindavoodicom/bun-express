{
  "name": "bun-express",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch --inspect=4000/debug src/server.ts",
    "start": "bun src/server.ts",
    "build": "bun build src/server.ts --target bun --outdir dist",
    "format": "bunx prettier --write .",
    "test": "jest",
    "prisma:init": "prisma init",
    "prisma:migrate": "prisma migrate dev --schema ./src/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema ./src/prisma/schema.prisma"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@prisma/client": "5.20.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.13",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.21.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "lint": "^0.8.19",
    "morgan": "^1.10.0",
    "prisma": "^5.20.0",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.5",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
